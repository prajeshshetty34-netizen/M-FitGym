# SECURITY CONFIGURATION TEMPLATE
# Copy this to .env and fill in your actual values

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# JWT Security
JWT_SECRET=your_super_secure_random_jwt_secret_here_minimum_32_characters

# Environment
NODE_ENV=production

# Frontend Origins (comma-separated for production)
FRONTEND_ORIGIN=https://your-vercel-domain.vercel.app,https://your-custom-domain.com

# Firebase Configuration (Server-side only)
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain
FIREBASE_PROJECT_ID=your_firebase_project_id